{
  "name": "Tampon",
  "version": "0.2",
  "manifest_version": 2,
  "description": "Stock up some great tweets and have them automatically shared throughout the day.",
  "browser_action": {
    "default_icon": "tampon.png",
    "default_title": "Tampon"
  },
  "background": {
    "scripts": ["listener.js"]
  },
  "permissions": [
    "tabs"
  ],
  "icons": {
    "48": "tampon_48.png",
    "128": "tampon_128.png"
  }
}